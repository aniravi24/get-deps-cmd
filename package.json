{
  "name": "get-deps-cmd",
  "version": "1.0.0",
  "description": "A simple way to get an install command from all your dependencies",
  "main": "dist/index.js",
  "bin": {
    "get-deps-cmd": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "start": "node dist/index.js",
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "yarn lint"
  },
  "repository": "git+https://github.com/aniravi24/get-deps-cmd.git",
  "keywords": [
    "dependencies",
    "command",
    "package.json"
  ],
  "contributors": [
    {
      "name": "Anirudh Ravichandran",
      "url": "https://github.com/aniravi24"
    },
    {
      "name": "Ethan Naluz",
      "url": "https://github.com/enaluz"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aniravi24/get-deps-cmd/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "!*.{js, ts}": [
      "pretty-quick --staged"
    ],
    "*.{js, ts}": [
      "organize-imports-cli",
      "pretty-quick --staged",
      "eslint --fix --ext .ts,.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false
}
